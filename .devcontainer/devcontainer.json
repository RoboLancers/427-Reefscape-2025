{
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu-24.04",
    "postCreateCommand": "cd /home/codespaces && wget 'https://github.com/wpilibsuite/vscode-wpilib/releases/download/v2025.2.1/vscode-wpilib-2025.2.1.vsix'",
   "features": {
     "ghcr.io/devcontainers/features/java:1": {
        "version": "none",
        "installGradle": "true",
        "installMaven": "true"
    }
  },
    "customizations": {
      "vscode": {
        "extensions": ["/home/codespaces/vscode-wpilib-2025.2.1.vsix","vscjava.vscode-java-pack","vscjava.vscode-gradle"]
      }
    }
}
